{
  "InstanceDnsServers": [
    "<%= node["eucalyptus"]["network"]["InstanceDnsServers"].join(",") %>"
  ],
  "Mido": {
    "Gateways": [
    <% @gateways.each_with_index do |gateway, index| -%>
      {<%= gateway.map{ |k,v| "\"#{k}\": \"#{v}\"" }.join(",") %>}<%= "," unless @gateways.size == index + 1 %>
    <% end %>
    ],
    "EucanetdHost": "<%= node["eucalyptus"]["network"]['EucanetdHost'] %>",
    "PublicGatewayIP": "<%= node["eucalyptus"]["network"]['PublicGatewayIP'] %>",
    "PublicNetworkCidr": "<%= node["eucalyptus"]["network"]['PublicNetworkCidr'] %>"
  },
  "Mode": "VPCMIDO",
  "PublicIps": [
  <% node["eucalyptus"]["network"]["PublicIps"].each do |value| -%>
    "<%= node["eucalyptus"]["network"]["PublicIps"].join(",") %>"
  <% end %>
  ]
}
